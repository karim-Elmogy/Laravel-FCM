{
    "name": "laravel/fcm",
    "description": "A Laravel package for FCM (Firebase Cloud Messaging) integration.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "google/apiclient": "^2.0",
        "illuminate/support": "^9.0 || ^10.0",
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "laravel\\fcm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "laravel\\fcm\\FCMServiceProvider"
            ]
        }
    }
}

