{
    "name": "elmogy/fcm",
    "description": "A Laravel package for FCM (Firebase Cloud Messaging) integration.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/karim-Elmogy/Laravel-FCM/issues",
        "source": "https://github.com/karim-Elmogy/Laravel-FCM"
    },
    "authors": [
        {
            "name": "Karim Elmogy",
            "email": "kimo72636@gmail.com"
        }
    ],
    "require": {
